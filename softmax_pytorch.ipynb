   ],
   "source": [
    "x = 10\n",
    "def softmax(x):\n",
    "    exponential = np.exp(x)\n",
    "    return np.divide(exponential, exponential.sum())\n",
    "print(softmax(x))\n",
    "\n",
    "print(\"\\nJ MANASA\")"
   ]
